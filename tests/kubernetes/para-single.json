{
  "settings": {
    "timeout": 1000
  },
  "relaychain": {
    "default_image": "docker.io/parity/polkadot:latest",
    "chain": "rococo-local",
    "default_command": "polkadot",
    "nodes": [
      {
        "name": "alice",
        "validator": true,
          "ws_port": 9944,
          "args": [
            "--alice",
            "-lparachain=debug",
            "--pool-limit=1000000000",
            "--pool-kbytes=10000000000"
          ]
      },
      {
        "name": "bob",
        "validator": true,
        "ws_port": 9944,
        "args": [
          "--alice",
          "-lparachain=debug",
          "--pool-limit=1000000000",
          "--pool-kbytes=10000000000"
        ]
      }
    ]
  },
  "parachains": [
    {
      "id": 1000,
      "cumulus_based": true,
      "collator": {
        "name": "collator01",
        "image": "parity/polkadot-collator",
        "command": "polkadot-collator",
        "args": [
          "-lparachain=debug"
        ]
      }
    }
  ]
}